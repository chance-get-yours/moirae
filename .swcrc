{
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": true,
        "decoratorsBeforeExport": true
      },
      "target": "es2020",
      "keepClassNames": true,
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      },
      "loose": false
    },
    "module": {
        "type": "commonjs",
        "noInterop": true,
        "strictMode": false,
        "lazy": true
    },
    "sourceMaps": true,
    "inlineSourcesContent": true,
    "minify": false
  }