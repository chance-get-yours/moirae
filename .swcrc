{
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": true,
        "decoratorsBeforeExport": true
      },
      "target": "es2017",
      "keepClassNames": true,
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      },
      "loose": false
    },
    "module": {
        "type": "commonjs",
        "noInterop": true
    },
    "sourceMaps": true,
    "inlineSourcesContent": true
  }