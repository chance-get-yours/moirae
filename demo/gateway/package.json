    "@moirae/core": "*",
    "rxjs": "^7.5.7",
    "@nestjs/websockets": "~9.1.6",
    "class-validator": "~0.13.2"
  },
  "devDependencies": {
    "@types/jest": "~29.2.0",
    "jest": "~29.2.2",
    "typescript": "~4.8.4"
  }
}
